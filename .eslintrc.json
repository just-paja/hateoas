{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "@optimics/eslint-config"
  ],
  "settings": {
    "react": {
      "version": "0.0.0"
    }
  },
  "rules": {
    "react/prop-types": 0
  },
  "overrides": [
    {
      "files": [
        "jest.afterEnv.js",
        "**/__jest__/**"
      ],
       "env": {
        "jest": true
      },
      "rules": {
        "import/extensions": ["error", "always"]
      }
    }
  ]
}
